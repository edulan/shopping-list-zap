{
  "name": "shopping_list_zap",
  "version": "1.0.0",
  "description": "Bot to automate the creation of weekly shopping list",
  "main": "src/index.js",
  "scripts": {
    "build-min": "browserify src/index.js -t [ babelify --presets [ es2015 ] ] --node | uglifyjs -c -o dist/index.min.js",
    "build": "browserify src/index.js -t [ babelify --presets [ es2015 ] ] --node -o dist/index.js",
    "watch": "watchify src/index.js -t [ babelify --presets [ es2015 ] ] --node -o dist/index.js -v",
    "start": "node dist/index.js",
    "test": "mocha --compilers js:babel-register",
    "test-watch": "mocha -w --compilers js:babel-register"
  },
  "author": "eduardo@elanchares.com",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint": "^2.4.0",
    "mocha": "^2.4.5",
    "uglify-js": "^2.6.2",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "markdown": "^0.5.0"
  }
}
